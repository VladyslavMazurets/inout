{
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "vueIndentScriptAndStyle": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
   "<THIRD_PARTY_MODULES>", 
    "^@/app/Components/(.*)$", 
    "^@/craftable-pro/Components/(.*)$", 
    "@/app/enums/(.*)$",
    "^@/app/types",
    "^@/craftable-pro/types/(.*)$",
    "^@/(.*)$",                            
    "^~/(.*)$",
    "^[./]" 
  ],
  "pluginSearchDirs": false,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true,
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "parser": "vue"
      }
    }
  ]
}